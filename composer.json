{
  "name": "ludicat/mtg-finder-sdk",
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": "^7.2.5 || ^8.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.8",
    "psr/http-client": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.36 || ^9.6.15"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Ludicat\\MtgFinder\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Ludicat\\MtgFinder\\": "tests/"
    }
  }
}
